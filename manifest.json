{
	"name": "CrittersBreak",
	"version": "0.1.0",
	"version_name": "Development",
	"manifest_version": 3,
	"description": "A browser addon to incentivise taking breaks feat. tiny critters!",
	"icons":{
	 "16": "icons/16x16.jpeg",
	 "32": "icons/32x32.jpeg",
	 "48": "icons/48x48.jpeg",
	 "128":"icons/128x128.jpeg"
	},
	"content_security_policy": {
	},
	"action":{
		"default_icon": "icons/16x16.jpeg",
		"default_popup":"popup/popup.html",
		"default_title":"CrittersBreak"
	},
	"background": {
		"service_worker": "eventPage.js"
	},
	"content_scripts":[
		{
			"matches": ["http://*/", "https://*/"],
			"js": ["content.js"]
		}
	],
	"permissions": [
	],
	"host_permissions": [
	],
	"web_accessible_resources":[{
		"resources": [],
		"matches": ["<all_urls>"]
	}]
}

