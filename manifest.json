{
	"name": "CrittersBreak",
	"version": "0.1.0",
	"version_name": "Development",
	"manifest_version": 3,
	"description": "A browser addon to incentivise taking breaks feat. tiny critters!",
	"icons":{
	 "16": "assets/critters/sir-teddy/idle/idle-1.png",
	 "32": "assets/critters/sir-teddy/idle/idle-1.png",
	 "48": "assets/critters/sir-teddy/idle/idle-1.png",
	 "128":"assets/critters/sir-teddy/idle/idle-1.png"
	},
	"content_security_policy": {
	},
	"action":{
		"default_icon": "assets/critters/sir-teddy/idle/idle-1.png",
		"default_popup":"popup/popup.html",
		"default_title":"CrittersBreak"
	},
	"background": {
		"service_worker": "eventPage.js"
	},
	"content_scripts":[
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"permissions": [
		"storage"
	],
	"host_permissions": [
	],
	"web_accessible_resources":[{
		"resources": ["assets/*"],
		"matches": ["<all_urls>"]
	}]
}

